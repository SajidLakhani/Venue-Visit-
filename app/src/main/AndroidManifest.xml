<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.banquetvisit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/SignUpActivityTheme">
        <activity android:name=".Customer_Activity"></activity>
        <activity android:name=".Manager_activity" />
        <activity android:name=".Caterer_details" />
        <activity android:name=".Photographer_details" />
        <activity android:name=".Venue_details" />
        <activity android:name=".Add_Photographers" />
        <activity android:name=".Add_caterers" />
        <activity android:name=".Add_venus" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>